    }
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "docs": "typedoc --options typedocconfig.ts",
    "prepublish": "npm run test && npm run clean && npm run build",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint --fix",
    "format": "prettier --write",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xanthous-tech/dgraph-orm.git"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/jest": "^24.0.23",
    "@types/uniqid": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.7",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.16.10",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.165",
    "@types/n3": "^1.1.1",
    "@xanthous/n3": "^1.3.5",
    "class-validator": "^0.10.2",
    "debug": "^4.1.1",
    "dgraph-js": "^2.0.2",
    "grpc": "^1.24.2",
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13",
    "uniqid": "^5.2.0"
  },
  "volta": {
    "node": "10.18.1",
    "yarn": "1.21.1"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  }
}
